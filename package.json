{
  "name": "template-substituter",
  "version": "1.0.1",
  "description": "Flexible way to evaluate EJS templates in files",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:release": "npm run clean && npm run lint && npm run build",
    "build:watch": "tsc --watch",
    "lint": "tslint -p tsconfig.json -e test/*.ts",
    "clean": "rimraf .nyc_output coverage documentation web index.js index.d.ts index.js.map test/*.js test/*.d.ts test/*.js.map lib/*.js lib/*.d.ts lib/*.js.map temp"
  },
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/chai": "^4.1.7",
    "@types/chalk": "^2.2.0",
    "@types/ejs": "^2.6.2",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.4",
    "@types/rimraf": "^2.0.2",
    "@types/yargs": "^12.0.9",
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3",
    "uglify-es": "^3.3.10"
  },
  "bin": {
    "substitude": "./bin/substitude.js"
  },
  "author": "Mark Albert",
  "license": "MIT",
  "dependencies": {
    "camelcase": "^5.0.0",
    "chalk": "^2.4.2",
    "ejs": "^2.6.1",
    "yargs": "^13.2.0"
  }
}
